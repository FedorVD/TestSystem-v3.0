<Window x:Class="TestSystem_v3._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestSystem_v3._0"
        mc:Ignorable="d"
        Title="MainWindow" Height="382" Width="431">
    <Grid>
        <Label Content="Login" HorizontalAlignment="Left" Height="39" Margin="108,90,0,0" VerticalAlignment="Top" Width="82" FontSize="18"/>
        <Label Content="Password" HorizontalAlignment="Left" Height="39" Margin="108,156,0,0" VerticalAlignment="Top" Width="82" FontSize="18"/>
        <Button x:Name="LoginButton" Content="Ok" HorizontalAlignment="Left" Margin="108,226,0,0" VerticalAlignment="Top" Height="56" Width="62" Click="LoginButton_Click" FontSize="18"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Height="56" Margin="203,226,0,0" VerticalAlignment="Top" Width="114" Click="CancelButton_Click" FontSize="18"/>
        <TextBox x:Name="LoginText" HorizontalAlignment="Left" Height="30" Margin="203,90,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="144" FontSize="18"/>
        <PasswordBox x:Name="PasswordText" HorizontalAlignment="Left" Height="30" Margin="203,157,0,0" VerticalAlignment="Top" Width="144" FontSize="18"/>

    </Grid>
</Window>
